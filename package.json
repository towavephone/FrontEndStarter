    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-react-proptypes": "^9.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-compat": "^2.1.0",
